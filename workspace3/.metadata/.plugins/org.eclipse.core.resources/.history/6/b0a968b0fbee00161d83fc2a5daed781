package dkawalsk_CSCI201L_Lecture_02_09_2017;

public class ThreadTest {
	
	public static void main(String args[]) {
		
	}

}

class MyThread extends Thread {
	private String name;
	public MyThread(String name) {
		this.name = name;
	}
	public void run() {
		for (int i = 0; i < 20; i++) {
			System.out.print(name + "-" + i + " ");
		}
		System.out.println();
	}
	
}