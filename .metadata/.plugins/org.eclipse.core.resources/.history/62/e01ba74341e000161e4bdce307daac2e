package DOM;

public class User {

	String username, password, fname, lname;
	Event[] feed;
	String[] following;
	
	public User () {}
	
	public User (String u, String p, String f, String l, Event[] events, String[] users)
	{
		username = u;
		password = p;
		fname = f;
		lname = l;
		feed = events;
		following = users;
	}
}
